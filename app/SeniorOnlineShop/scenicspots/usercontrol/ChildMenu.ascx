<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="ChildMenu.ascx.cs" Inherits="SeniorOnlineShop.scenicspots.usercontrol.ChildMenu" %>
<p class="more more03"><span id="sCurrMenu"></span>您现在所在位置：<a id="aDefault">首页</a> >> <a id="aCurrMenu" style="cursor:pointer;"></a></p><script type="text/javascript">    $("#parentMenu :a").each(function(i){        if(i==1)        {            $("#aDefault").attr("href",$(this).attr("href"));          }        if($(this).attr("class")=="menu-on")        {            $("#sCurrMenu").text($(this).text());            $("#aCurrMenu").attr("href",$(this).attr("href")).text($(this).text());          }    });</script>